<!DOCTYPE html>
<html lang="<%= config.language %>">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="profile" href="http://gmpg.org/xfn/11">
  <link rel="pingback" href="<%= config.url %>/pingback">
  <%- partial('_partial/head') %>
</head>

<body class="<%= body_class %>">
  <div class="sp-header"><%- partial('_partial/sportspress-header') %></div>
  <div id="page" class="hfeed site">
    <a class="skip-link screen-reader-text" href="#content">Skip to content</a>

    <header id="masthead" class="site-header" role="banner">
      <div class="header-wrapper">
        <%- partial('_partial/header-area') %>
      </div><!-- .header-wrapper -->
    </header><!-- #masthead -->

    <div id="content" class="site-content">
      <div class="content-wrapper">
        <%- partial('_partial/before-template') %>
        <%- body %>
      </div><!-- .content-wrapper -->
    </div><!-- #content -->

    <footer id="colophon" class="site-footer" role="contentinfo">
      <div class="footer-wrapper">
        <div class="footer-area">
          <div class="footer-inner">
            <div id="quaternary" class="footer-widgets" role="complementary">
              <% for (let i = 1; i <= 3; i++) { %>
                <div class="footer-widget-region"><%- partial('_partial/footer-widget-' + i) %></div>
              <% } %>
            </div>
          </div><!-- .footer-inner -->
        </div><!-- .footer-area -->
      </div><!-- .footer-wrapper -->
    </footer><!-- #colophon -->
  </div><!-- #page -->

  <div class="site-info">
    <div class="info-wrapper">
      <div class="info-area">
        <div class="info-inner">
          <%- partial('_partial/footer') %>
        </div><!-- .info-inner -->
      </div><!-- .info-area -->
    </div><!-- .info-wrapper -->
  </div><!-- .site-info -->

  <%- partial('_partial/footer') %>
</body>
</html> 